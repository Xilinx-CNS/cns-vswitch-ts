#!/bin/bash
# SPDX-License-Identifier: Apache-2.0
# (c) Copyright 2016 - 2022 Xilinx, Inc. All rights reserved.
pushd vswitch-ts >/dev/null

help() {
    cat <<EOF
This is a wrapper script which builds the autogenerated part of the
Test Environment documentation.

Usage:
  $ gen_doxygen

Documentation will be located at vswitch-ts/doc/html/
EOF

    exit 0
}

[ "x$1" = "x--help" ] || [ "x$1" = "x-h" ] && help

"${TE_BASE}"/scripts/dox_process vswitch "vSwitch Tests"

doxygen > /dev/null

echo "" >&2
echo "See file://$(pwd)/doc/html/index.html" >&2

popd >/dev/null
